<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">


<!--Main frame-->
<Frame name="BagChamp" parent="UIParent" inherits="PortraitFrameTemplate">
    <Size x="484" y="512"/>
    <Anchors>
        <Anchor point="BOTTOMRIGHT" x="-64" y="128"/>
    </Anchors>
    <Layers>
        <!-- Background LAYER -->
        <Layer level="BACKGROUND">

        </Layer>

        <!-- Overlay LAYER -->
        <Layer level="OVERLAY">        
            <!-- Title -->
            <FontString name="$parent_Title" parentKey="title" inherits="GameFontNormal" text="BagChamp">
                <Anchors>
                    <Anchor point="TOP">
                        <Offset x="0" y="-5"/>
                    </Anchor>
                </Anchors>
            </FontString>

            <!-- Bag portrait in corner -->
            <Texture name="$parent_Portrait" parentKey="portrait" file="Interface\ICONS\INV_Misc_Bag_22">
                <Size x="58" y="58"/>
                <Anchors>
                    <Anchor point="TOPLEFT">
                        <Offset x="-4" y="7" />
                    </Anchor>
                </Anchors>
            </Texture>
        </Layer>
    </Layers>
    <Scripts>
        <OnLoad function="BagChamp_OnLoad"/>
    </Scripts>

</Frame>



</Ui>
